{
  "name": "yantr",
  "type": "module",
  "scripts": {
    "server": "node daemon/main.js",
    "check": "node check.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docker": "docker stop yantr || true && docker rm yantr || true && docker build -t yantr:dev . && docker run -d --name yantr -p 5252:5252 -v /var/run/docker.sock:/var/run/docker.sock -v /var/lib/docker/volumes:/var/lib/docker/volumes yantr:dev && docker logs -f yantr",
    "website": "npx @tailwindcss/cli -i ./website/tailwind.input.css -o ./website/tailwind.css --minify && node website-build.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "apexcharts": "^5.3.6",
    "cors": "^2.8.5",
    "dockerode": "^4.0.9",
    "express": "^4.21.2",
    "lucide-vue-next": "^0.487.0",
    "nunjucks": "^3.2.4",
    "tailwindcss": "^4.1.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.10.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@vitejs/plugin-vue": "^5.2.3",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2"
  }
}
