    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-apexcharts": "^1.10.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
